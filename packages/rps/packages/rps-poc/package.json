{
  "name": "rps",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "module": "src/index.js",
  "dependencies": {
    "@firebase/app-types": "^0.3.2",
    "@types/node": "^10.5.4",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.5",
    "@types/web3": "^1.0.2",
    "ajv": "^6.5.1",
    "aphrodite": "^2.2.2",
    "autoprefixer": "7.1.6",
    "babel-core": "6",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv-expand": "4.2.0",
    "enum": "^2.5.0",
    "firebase": "^5.0.4",
    "fmg-core": "^0.0.6",
    "fmg-simple-adjudicator": "^0.0.6",
    "fs-extra": "3.0.1",
    "key-mirror": "^1.0.1",
    "lodash": "^4.17.10",
    "nanoid": "^1.0.3",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.3.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-saga-firebase": "^0.14.0",
    "resolve": "1.6.0",
    "sha3": "^1.2.2",
    "truffle-contract": "^3.0.6",
    "web3": "^1.0.0-beta.34",
    "web3-detect-network": "^0.0.17",
    "web3-utils": "^1.0.0-beta.30",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "config":{
    "deploymentNetwork" : "ropsten"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "run-s test:truffle 'test:app --all'",
    "test:ci": "run-s build test:truffle 'test:app --all --ci'",
    "test:app": "node scripts/test.js --env=jsdom",
    "test:truffle": "node scripts/test_truffle.js",
    "truffle:migrate": "node_modules/.bin/truffle migrate --network $npm_package_config_deploymentNetwork",
    "postinstall": "rm -f node_modules/web3/index.d.ts"
  },
  "devDependencies": {
    "@babel/runtime": "^7.0.0-beta.53",
    "@types/jest": "^23.3.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "copyfiles": "^2.0.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "0.4.9",
    "ganache-cli": "^6.1.8",
    "html-webpack-plugin": "2.29.0",
    "jest": "^23.5.0",
    "npm-run-all": "^4.1.3",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "redux-saga-test-plan": "^3.7.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "truffle": "^5.0.0-beta.0",
    "truffle-hdwallet-provider": "^1.0.0-web3one.0",
    "truffle-solidity-loader": "^0.1.0",
    "ts-jest": "23",
    "ts-loader": "^2.3.7",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^17.0.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
