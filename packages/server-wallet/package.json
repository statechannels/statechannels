  "lint-staged": {
    "src/**/*.ts": "eslint --max-warnings=0"
  },
  "main": "lib/src/index.js",
  "scripts": {
    "db:create": "createdb $SERVER_DB_NAME -U postgres",
    "db:drop": "dropdb $SERVER_DB_NAME -U postgres",
    "db:migrate": "npx knex migrate:latest --cwd src/db-admin",
    "db:rollback": "npx knex migrate:rollback --cwd src/db-admin",
    "db:seed": "npx knex seed:run --cwd src/db-admin",
    "db:setup:dev": "yarn db:drop && yarn db:create && yarn db:migrate",
    "db:reset": "yarn db:drop; yarn db:create && yarn db:migrate",
    "db:reset:test": "(NODE_ENV=test yarn db:reset)",
    "generate-api": "yarn api-extractor run --local",
    "lint:check": "eslint \"*/**/*.{js,ts}\" --cache",
    "lint:write": "eslint \"*/**/*.{js,ts}\" --fix",
    "precommit": "lint-staged --quiet",
    "prepare": "rm -rf lib; tsc -b . && node ./scripts/inject-wallet-version.js",
    "prestart": "yarn prepare && npm run db:rollback && npm run db:migrate && npm run db:seed",
    "prettier:check": "npx prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:write": "npx prettier --write 'src/**/*.{ts,tsx}'",
    "profile": "./e2e-test/scripts/profile.sh",
    "start:shared-ganache": "NODE_ENV=development npx start-shared-ganache",
    "test": "yarn jest -c ./jest/jest.config.js --runInBand",
    "test:chain": "jest -c ./jest/jest.chain.config.js --runInBand",
    "test:ci": "yarn run-p 'test --bail' 'test:chain --bail'",
    "test:e2e": "./src/test_db.sh server_wallet_payer server_wallet_receiver; yarn tsc; yarn jest -c ./jest/jest.e2e.config.js --runInBand",
    "test:stress": "./src/test_db.sh server_wallet_payer server_wallet_receiver; yarn tsc; NODE_ENV=test npx ts-node ./e2e-test/scripts/stress-test.ts",
    "test:with-peers": "yarn jest -c ./jest/jest.with-peers.config.js --runInBand"
  },
  "types": "lib/src/index.d.ts"
}
