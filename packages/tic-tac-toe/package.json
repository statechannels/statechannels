{
  "name": "@statechannels/tic-tac-toe",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build:ci": "run-s clearContracts contracts:compile",
    "build:webpack": "CI=false node scripts/build.js",
    "build": "run-s clearContracts build:webpack",
    "clearContracts": "rm -rf ./build/contracts",
    "contracts:compile": "node ./bin/compile.js",
    "prettier:check": "prettier --check '**/*.{scss,json}' --ignore-path '.gitignore'",
    "prettier:write": "prettier --write '**/*.{scss,json}' --ignore-path '.gitignore'",
    "lint:check": "run-s eslint:check prettier:check",
    "lint:write": "run-s eslint:write prettier:write",
    "eslint:check": "eslint \"**/*.{js,ts}\" --cache --max-warnings=0",
    "eslint:write": "eslint \"**/*.{js,ts}\" --fix --max-warnings=0",
    "start": "yarn contracts:compile",
    "start:shared-ganache": "NODE_ENV=development npx start-shared-ganache",
    "prepare": "yarn build:ci",
    "test:prepare": "yarn clearContracts && yarn contracts:compile",
    "test:contracts": "npx jest -c ./config/jest/jest.contracts.config.js",
    "test": "yarn test:contracts",
    "test:ci": "yarn test:ci:contracts",
    "test:ci:contracts": "yarn test:contracts --all --ci --runInBand --bail",
    "precommit": "lint-staged --quiet"
  },
  "lint-staged": {
    "**/*.{js,ts}": "eslint --cache",
    "**/*.{json,scss}": "prettier --ignore-path '.gitignore'"
  },
  "dependencies": {
    "@babel/core": "^7.6.2",
