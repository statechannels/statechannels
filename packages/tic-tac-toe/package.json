    "@types/detect-port": "1.1.0",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/eslint": "^6.1.3",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/fs-extra": "3.0.1",
    "@types/jest": "24.0.18",
