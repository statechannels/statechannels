    "ajv": "^6.5.1",
    "aphrodite": "^2.2.2",
    "autoprefixer": "^9.1.5",
    "babel-preset-react-app": "^9.0.2",
    "babel-runtime": "^6.23.0",
    "bn.js": "^4.11.8",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv-extended": "2.5.0",
    "enum": "^2.5.0",
    "ethers": "^4.0.39",
    "firebase": "^7.5.2",
    "fmg-core": "0.5.9",
