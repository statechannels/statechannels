  "version": "0.0.1",
  "author": "Alex Gap",
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@rimble/connection-banner": "1.2.3",
    "@rimble/utils": "1.2.3",
    "@sentry/browser": "5.15.5",
